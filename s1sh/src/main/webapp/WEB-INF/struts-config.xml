<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://struts.apache.org/dtds/struts-config_1_3.dtd">

<struts-config>
	<form-beans>
		<form-bean name="LoginForm" type="cn.zain.form.LoginForm"></form-bean>
	</form-beans>
	<global-exceptions />
	<global-forwards />
	<action-mappings>
		<action path="/login" name="LoginForm" type="cn.zain.action.LoginAction"
			scope="request" input="/index.jsp"> 
			<forward name="success" path="/success.jsp"></forward>
		</action>


	</action-mappings>
	<!--  国际化文件配置
	<message-resources parameter="com.yza.struts.ApplicationResources" />-->
	<message-resources parameter="ApplicationResources" />

</struts-config>

