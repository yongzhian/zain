<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.zain.model.po.SysRole" table="sys_role" schema="luman">
        <id name="id">
            <column name="id" sql-type="bigint(20)"/>
        </id>
        <property name="roleCode">
            <column name="role_code" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="roleName">
            <column name="role_name" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="isValid">
            <column name="is_valid" sql-type="char(1)" length="1"/>
        </property>
        <property name="createTime">
            <column name="create_time" sql-type="timestamp"/>
        </property>
        <set name="users" inverse="true" table="sys_user_role" schema="luman" lazy="false">
            <key>
                <column name="sys_role_id" not-null="true"/>
            </key>
            <many-to-many not-found="ignore" class="cn.zain.model.po.SysUser">
                <column name="sys_user_id" not-null="true"/>
            </many-to-many>
        </set>
        <set name="nodes" table="sys_role_node" schema="luman" lazy="false">
            <key>
                <column name="sys_role_id"/>
            </key>
            <many-to-many not-found="ignore" class="cn.zain.model.po.SysNode">
                <column name="sys_node_id"/>
            </many-to-many>
        </set>
    </class>
</hibernate-mapping>